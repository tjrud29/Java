package com.kh.scanner;
import java.until.*;
import java.util.Scanner;

public class scanner {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("나이를 입력해주세요.");
		int age = sc.nextInt();
		
		System.out.println("내 나이는 " + age + "세 입니다.");
		
	}
}
